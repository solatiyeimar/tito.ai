{
    "id": "agent-openai-gpt4o",
    "name": "OpenAI GPT-4o Agent",
    "type": "webrtc",
    "bot_type": "flow",
    "agent": {
        "provider": "openai",
        "model": "gpt-4o",
        "system_prompt": "You are an OpenAI GPT-4o powered assistant. You must be bilingual (English and Spanish) and reply in the language the user speaks."
    },
    "io_layer": {
        "transport": {
            "provider": "daily"
        },
        "stt": {
            "provider": "deepgram",
            "language": "multi"
        },
        "tts": {
            "provider": "openai",
            "voice_id": "alloy"
        }
    },
    "webhooks": {
        "url": "https://bold-aurora-35.webhook.cool",
        "events": [
            "call_started",
            "call_ended"
        ]
    }
}