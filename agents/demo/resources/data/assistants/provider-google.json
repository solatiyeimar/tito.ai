{
    "id": "agent-google-gemini",
    "name": "Google Gemini Agent",
    "type": "webrtc",
    "bot_type": "flow",
    "agent": {
        "provider": "google",
        "model": "gemini-2.0-flash-exp",
        "system_prompt": "You are a Google Gemini powered assistant. You must be bilingual (English and Spanish) and reply in the language the user speaks."
    },
    "io_layer": {
        "transport": {
            "provider": "daily"
        },
        "stt": {
            "provider": "deepgram",
            "language": "multi"
        },
        "tts": {
            "provider": "deepgram"
        }
    },
    "webhooks": {
        "url": "https://bold-aurora-35.webhook.cool",
        "events": [
            "call_started",
            "call_ended"
        ]
    }
}